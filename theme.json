{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"templateParts": [
		{
			"name": "header",
			"area": "header"
		},
		{
			"name": "footer",
			"area": "footer"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": false,
			"link": false,
			"defaultPalette": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"customDuotone": false,
			"customGradient": false,
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"name": "Black",
					"color": "#000000",
					"slug": "black"
				},
				{
					"name": "Dark Gray",
					"color": "#555555",
					"slug": "dark-gray"
				},
				{
					"name": "Medium Gray",
					"color": "#9999a0",
					"slug": "medium-gray"
				},
				{
					"name": "Gray",
					"color": "#c1c4ca",
					"slug": "gray"
				},
				{
					"name": "Light Gray",
					"color": "#e3e3e3",
					"slug": "light-gray"
				},
				{
					"name": "Lightest Gray",
					"color": "#f5f5f5",
					"slug": "lightest-gray"
				},
				{
					"name": "White",
					"color": "#ffffff",
					"slug": "white"
				},
				{
					"name": "Primary blue",
					"color": "#003c6c",
					"slug": "ucsc-primary-blue"
				},
				{
					"name": "Primary Yellow",
					"color": "#fdc700",
					"slug": "ucsc-primary-yellow"
				},
				{
					"name": "Secondary blue",
					"color": "#006aad",
					"slug": "ucsc-secondary-blue"
				},
				{
					"name": "Pacific Blue",
					"color": "#13a5dc",
					"slug": "ucsc-pacific-blue"
				},
				{
					"name": "Ocean Blue",
					"color": "#007988",
					"slug": "ucsc-ocean-blue"
				},
				{
					"name": "Orange",
					"color": "#f29813",
					"slug": "ucsc-orange"
				},
				{
					"name": "Green",
					"color": "#93c02d",
					"slug": "ucsc-green"
				},
				{
					"name": "Rubine Red",
					"color": "#da216d",
					"slug": "ucsc-rubine-red"
				}
			]
		},
		"layout": {
			"contentSize": "72rem"
		},
		"spacing": {
			"blockGap": true,
			"padding": true,
			"margin": true,
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"typography": {
			"lineHeight": true,
			"customFontSize": false,
			"fontSizes": [
				{
					"slug": "small",
					"size": "clamp(0.95rem, -0.13vw + 0.98rem, 0.88rem)",
					"name": "Small"
				},
				{
					"slug": "base",
					"size": "clamp(1rem, 0.23vw + 0.95rem, 1.13rem)",
					"name": "Base"
				},
				{
					"slug": "medium",
					"size": "clamp(1.05rem, 0.71vw + 0.91rem, 1.44rem)",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "clamp(1.1rem, 1.35vw + 0.83rem, 1.84rem)",
					"name": "Large"
				},
				{
					"slug": "x-large",
					"size": "clamp(1.16rem, 2.18vw + 0.72rem, 2.36rem)",
					"name": "X-Large"
				},
				{
					"slug": "xx-large",
					"size": "clamp(1.22rem, 3.28vw + 0.56rem, 3.02rem)",
					"name": "XX-Large"
				},
				{
					"slug": "xxx-large",
					"size": "clamp(1.28rem, 4.71vw + 0.33rem, 3.87rem)",
					"name": "XXX-Large"
				}
			],
			"fontFamilies": [
				{
					"fontFamily": "Roboto,-apple-system,BlinkMacSystemFont,\"Segoe UI\",Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"slug": "roboto",
					"name": "Roboto"
				},
				{
					"fontFamily": "\"Roboto Condensed\", sans-serif",
					"slug": "roboto-condensed",
					"name": "Roboto Condensed"
				}
			]
		},
		"blocks": {
			"core/group": {
				"color": {
					"background": true
				}
			},
			"core/column": {
				"color": {
					"background": true
				}
			},
			"core/columns": {
				"color": {
					"background": true
				}
			},
			"core/separator": {
				"color": {
					"background": true
				}
			},
			"core/button": {
				"color": {
					"custom": false,
					"palette": [
						{
							"slug": "white",
							"color": "#fff",
							"name": "White"
						},
						{
							"slug": "black",
							"color": "#000",
							"name": "Black"
						},
						{
							"slug": "ucsc-primary-blue",
							"color": "#003c6c",
							"name": "Primary blue"
						},
						{
							"slug": "ucsc-green",
							"color": "#93C02D",
							"name": "Green"
						},
						{
							"slug": "ucsc-primary-yellow",
							"color": "#fdc700",
							"name": "Primary yellow"
						},
						{
							"name": "Orange",
							"color": "#f29813",
							"slug": "ucsc-orange"
						},
						{
							"name": "Danger Red",
							"color": "#bb2f2f",
							"slug": "ucsc-danger-red"
						},
						{
							"name": "Success Green",
							"color": "#417b14",
							"slug": "ucsc-success-green"
						}
					]
				},
				"border": {
					"radius": false
				}
			},
			"core/paragraph": {
				"color": {
					"custom": false,
					"background": true,
					"text": true,
					"link": true
				},
				"typography": {
					"customFontSize": true
				}
			},
			"core/navigation": {
				"typography": {
					"customFontSize": true
				}
			},
			"core/heading": {
				"typography": {
					"customFontSize": false
				},
				"color": {
					"custom": false,
					"background": false
				}
			}
		},
		"custom": {
			"line-height": {
				"tiny": 1.15,
				"small": 1.3,
				"baseline": 1.5,
				"medium": 1.68,
				"large": 1.88
			},
			"spacing": {
				"x-small": ".5rem",
				"small": ".85rem",
				"baseline": "1.25rem",
				"medium": "1.65rem",
				"large": "2rem",
				"outer": "var(--wp--custom--spacing--small, 1.75rem)"
			},
			"border": {
				"radius": "0"
			},
			"color": {
				"link": "var(--wp--preset--color--ucsc-primary-blue)",
				"link-decoration": "var(--wp--preset--color--ucsc-primary-yellow)",
				"site-title": "var(--wp--preset--color--black)"
			}
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "var(--wp--custom--spacing--medium)"
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--custom--color--default-text)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--roboto)",
			"fontSize": "var(--wp--preset--font-size--base)",
			"lineHeight": "var(--wp--custom--line-height--baseline)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)",
					"fontWeight": "400"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--line-height--tiny)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--line-height--tiny)"
				}
			}
		},
		"blocks": {
			"core/heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)"
				},
				"color": {
					"text": "var(--wp--custom--color--black)"
				},
				"spacing": {
					"margin": {
						"top": ".12em",
						"right": "0",
						"bottom": "0.25em",
						"left": "0"
					}
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--line-height--small)",
					"fontSize": "2.8rem"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"color": {
					"text": "var(--wp--preset--color--light-gray)"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"right": "0",
						"bottom": "0.5em",
						"left": "0"
					}
				}
			},
			"core/post-title": {
				"typography": {
					"lineHeight": "var(--wp--custom--line-height--baseline)"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"right": "0",
						"bottom": ".25rem",
						"left": "0"
					}
				}
			},
			"core/columns": {
				"spacing": {
					"margin": {
						"top": "0.75em",
						"right": "0",
						"bottom": "0.75em",
						"left": "0"
					}
				}
			},
			"core/block-post": {
				"spacing": {
					"margin": {
						"top": "0"
					}
				}
			},
			"core/button": {
				"border": {
					"radius": "var(--wp--custom--border--radius)"
				}
			}
		}
	},
	"customTemplates": []
}
