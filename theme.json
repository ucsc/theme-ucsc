{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"templateParts": [
		{
			"name": "header",
			"area": "header"
		},
		{
			"name": "footer",
			"area": "footer"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": false,
			"link": false,
			"defaultPalette": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"customDuotone": false,
			"customGradient": false,
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"name": "Black",
					"color": "#000000",
					"slug": "black"
				},
				{
					"name": "Dark Gray",
					"color": "#555555",
					"slug": "dark-gray"
				},
				{
					"name": "Medium Gray",
					"color": "#9999a0",
					"slug": "medium-gray"
				},
				{
					"name": "Gray",
					"color": "#c1c4ca",
					"slug": "gray"
				},
				{
					"name": "Light Gray",
					"color": "#e3e3e3",
					"slug": "light-gray"
				},
				{
					"name": "Lightest Gray",
					"color": "#f5f5f5",
					"slug": "lightest-gray"
				},
				{
					"name": "White",
					"color": "#ffffff",
					"slug": "white"
				},
				{
					"name": "Primary blue",
					"color": "#003c6c",
					"slug": "ucsc-primary-blue"
				},
				{
					"name": "Primary Yellow",
					"color": "#fdc700",
					"slug": "ucsc-primary-yellow"
				},
				{
					"name": "Secondary blue",
					"color": "#006aad",
					"slug": "ucsc-secondary-blue"
				},
				{
					"name": "Pacific Blue",
					"color": "#13a5dc",
					"slug": "ucsc-pacific-blue"
				},
				{
					"name": "Ocean Blue",
					"color": "#007988",
					"slug": "ucsc-ocean-blue"
				},
				{
					"name": "Orange",
					"color": "#f29813",
					"slug": "ucsc-orange"
				},
				{
					"name": "Green",
					"color": "#93c02d",
					"slug": "ucsc-green"
				},
				{
					"name": "Rubine Red",
					"color": "#da216d",
					"slug": "ucsc-rubine-red"
				}
			]
		},
		"layout": {
			"contentSize": "72rem"
		},
		"spacing": {
			"blockGap": true,
			"padding": true,
			"margin": true,
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"typography": {
			"lineHeight": true,
			"customFontSize": false,
			"fontSizes": [
				{
					"slug": "small",
					"size": "clamp(0.79rem, 1.125vw, 1rem)",
					"name": "Small"
				},
				{
					"slug": "base",
					"size": "clamp(1rem, 1.266vw, 1.266rem)",
					"name": "Base"
				},
				{
					"slug": "medium",
					"size": "clamp(1.266rem, 1.602vw, 1.424rem)",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "clamp(1.424rem, 2.027vw, 1.802rem)",
					"name": "Large"
				},
				{
					"slug": "x-large",
					"size": "clamp(1.802rem, 2.566vw, 2.281rem)",
					"name": "X-Large"
				},
				{
					"slug": "xx-large",
					"size": "clamp(2.281rem, 3.247vw, 2.566rem)",
					"name": "XX-Large"
				},
				{
					"slug": "xxx-large",
					"size": "clamp(2.887rem, 4.11vw, 2.887rem)",
					"name": "XXX-Large"
				}
			],
			"fontFamilies": [
				{
					"fontFamily": "Roboto,-apple-system,BlinkMacSystemFont,\"Segoe UI\",Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"slug": "roboto",
					"name": "Roboto"
				},
				{
					"fontFamily": "\"Roboto Serif\", serif",
					"slug": "roboto-serif",
					"name": "Roboto Serif"
				}
			]
		},
		"blocks": {
			"core/group": {
				"color": {
					"background": true
				}
			},
			"core/column": {
				"color": {
					"background": true
				}
			},
			"core/columns": {
				"color": {
					"background": true
				}
			},
			"core/separator": {
				"color": {
					"background": true
				}
			},
			"core/button": {
				"color": {
					"custom": false,
					"palette": []
				},
				"border": {
					"radius": false
				}
			},
			"core/paragraph": {
				"color": {
					"custom": false,
					"background": true,
					"text": true,
					"link": true
				},
				"typography": {
					"customFontSize": true
				}
			},
			"core/navigation": {
				"typography": {
					"customFontSize": true
				}
			},
			"core/heading": {
				"typography": {
					"customFontSize": false
				},
				"color": {
					"custom": false,
					"background": false
				}
			}
		},
		"custom": {
			"line-height": {
				"tiny": 1.125,
				"small": 1.266,
				"baseline": 1.602,
				"medium": 1.802,
				"large": 2.027
			},
			"spacing": {
				"x-small": ".555rem",
				"small": ".889rem",
				"baseline": "1.266rem",
				"medium": "1.802rem",
				"large": "2.281rem",
				"outer": "var(--wp--custom--spacing--small, 1.802rem)"
			},
			"border": {
				"radius": "0"
			},
			"color": {
				"link": "var(--wp--preset--color--ucsc-primary-blue)",
				"link-decoration": "var(--wp--preset--color--ucsc-primary-yellow)"
			}
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "var(--wp--custom--spacing--medium)"
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--black)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--roboto)",
			"fontSize": "var(--wp--preset--font-size--base)",
			"lineHeight": "var(--wp--custom--line-height--baseline)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			}
		},
		"blocks": {
			"core/heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)"
				},
				"color": {
					"text": "var(--wp--custom--color--black)"
				},
				"spacing": {
					"margin": {
						"top": ".702em",
						"right": "0",
						"bottom": "0.493em",
						"left": "0"
					}
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--line-height--small)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--small)"
				},
				"color": {
					"text": "var(--wp--preset--color--light-gray)"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"right": "0",
						"bottom": "0.555em",
						"left": "0"
					}
				}
			},
			"core/post-title": {
				"typography": {
					"lineHeight": "var(--wp--custom--line-height--baseline)"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"right": "0",
						"bottom": ".493rem",
						"left": "0"
					}
				}
			},
			"core/columns": {
				"spacing": {
					"margin": {
						"top": "0.493rem",
						"right": "0",
						"bottom": "0.642rem",
						"left": "0"
					}
				}
			},
			"core/block-post": {
				"spacing": {
					"margin": {
						"top": "0"
					}
				}
			},
			"core/button": {
				"border": {
					"radius": "var(--wp--custom--border--radius)"
				}
			}
		}
	},
	"customTemplates": []
}
