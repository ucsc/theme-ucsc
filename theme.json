{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"templateParts": [
		{
			"name": "header",
			"area": "header"
		},
		{
			"name": "footer",
			"area": "footer"
		}
	],
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"color": {
			"custom": false,
			"link": true,
			"defaultPalette": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"customDuotone": false,
			"customGradient": true,
			"duotone": [],
			"gradients": [
				{
					"name": "UCSC Headline Gradient",
					"gradient": "linear-gradient(90deg, #3e34b2 0%, #1569a9 50%, #79b9ad 100%)",
					"slug": "ucsc-headline-gradient"
				},
				{
					"name": "UCSC Header Gradient",
					"gradient": "linear-gradient(90deg, #003c6c 29%, #1569a9 100%)",
					"slug": "ucsc-header-gradient"
				}
			],
			"palette": [
				{
					"name": "Black",
					"color": "#000000",
					"slug": "black"
				},
				{
					"name": "Light Gray",
					"color": "#d9d9d9",
					"slug": "light-gray"
				},
				{
					"name": "Lightest Gray",
					"color": "#f5f5f5",
					"slug": "lightest-gray"
				},
				{
					"name": "White",
					"color": "#ffffff",
					"slug": "white"
				},
				{
					"name": "Primary Blue",
					"color": "#003c6c",
					"slug": "ucsc-primary-blue"
				},
				{
					"name": "Primary Yellow",
					"color": "#fdc700",
					"slug": "ucsc-primary-yellow"
				},
				{
					"name": "Secondary blue",
					"color": "#006aad",
					"slug": "ucsc-secondary-blue"
				},
				{
					"name": "Pacific Blue",
					"color": "#13a5dc",
					"slug": "ucsc-pacific-blue"
				},
				{
					"name": "Ocean Blue",
					"color": "#007988",
					"slug": "ucsc-ocean-blue"
				},
				{
					"name": "University Yellow",
					"color": "#ffbf00",
					"slug": "ucsc-university-yellow"
				},
				{
					"name": "Orange",
					"color": "#f29813",
					"slug": "ucsc-orange"
				},
				{
					"name": "Green",
					"color": "#93c02d",
					"slug": "ucsc-green"
				},
				{
					"name": "Persimmon",
					"color": "#ff6666",
					"slug": "ucsc-persimmon"
				},
				{
					"name": "Rubine Red",
					"color": "#da216d",
					"slug": "ucsc-rubine-red"
				},
				{
					"name": "Violator Gold",
					"color": "#997404",
					"slug": "ucsc-violator-gold"
				},
				{
					"name": "Violator Yellow",
					"color": "#fdf3d1",
					"slug": "ucsc-violator-yellow"
				},
				{
					"name": "Violator Pink",
					"color": "#f3d8da",
					"slug": "ucsc-violator-pink"
				},
				{
					"name": "Violator Green",
					"color": "#d5e6de",
					"slug": "ucsc-violator-green"
				},
				{
					"name": "UCSC Royal Blue",
					"color": "#1569a9",
					"slug": "ucsc-royal-blue"
				},
				{
					"name": "UCSC Rubine Blue",
					"color": "#0a3355",
					"slug": "ucsc-rubine-blue"
				}
			]
		},
		"layout": {
			"contentSize": "80rem"
		},
		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "clamp(1rem, 2vw, 1.5rem)",
					"slug": "20",
					"name": "1"
				},
				{
					"size": "clamp(1.5rem, 5vw, 2rem)",
					"slug": "30",
					"name": "2"
				},
				{
					"size": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)",
					"slug": "40",
					"name": "3"
				},
				{
					"size": "clamp(2.5rem, 8vw, 4.5rem)",
					"slug": "50",
					"name": "4"
				},
				{
					"size": "clamp(3.75rem, 10vw, 7rem)",
					"slug": "60",
					"name": "5"
				},
				{
					"size": "clamp(5rem, 5.25rem + ((1vw - 0.48rem) * 9.096), 8rem)",
					"slug": "70",
					"name": "6"
				},
				{
					"size": "clamp(7rem, 14vw, 11rem)",
					"slug": "80",
					"name": "7"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fluid": true,
			"lineHeight": true,
			"customFontSize": false,
			"fontSizes": [
				{
					"slug": "small",
					"size": ".833rem",
					"name": "Small",
					"fluid": {
						"min": ".79rem",
						"max": ".833rem"
					}
				},
				{
					"slug": "base",
					"size": "1rem",
					"name": "Base",
					"fluid": {
						"min": ".889rem",
						"max": "1rem"
					}
				},
				{
					"slug": "one",
					"size": "1.2rem",
					"name": "1",
					"fluid": {
						"min": "1rem",
						"max": "1.2rem"
					}
				},
				{
					"slug": "two",
					"size": "1.44rem",
					"name": "2",
					"fluid": {
						"min": "1.125rem",
						"max": "1.44rem"
					}
				},
				{
					"slug": "three",
					"size": "1.728rem",
					"name": "3",
					"fluid": {
						"min": "1.266rem",
						"max": "1.728rem"
					}
				},
				{
					"slug": "four",
					"size": "2.074rem",
					"name": "4",
					"fluid": {
						"min": "1.424rem",
						"max": "2.074rem"
					}
				},
				{
					"slug": "five",
					"size": "2.488rem",
					"name": "5",
					"fluid": {
						"min": "1.602rem",
						"max": "2.488rem"
					}
				},
				{
					"slug": "six",
					"size": "2.986rem",
					"name": "6",
					"fluid": {
						"min": "1.802rem",
						"max": "2.986rem"
					}
				},
				{
					"slug": "seven",
					"size": "3.583rem",
					"name": "7",
					"fluid": {
						"min": "2.027rem",
						"max": "3.583rem"
					}
				},
				{
					"slug": "eight",
					"size": "5.16rem",
					"name": "8",
					"fluid": {
						"min": "2.566rem",
						"max": "5.16rem"
					}
				}
			],
			"fontFamilies": [
				{
					"fontFamily": "Roboto,-apple-system,BlinkMacSystemFont,\"Segoe UI\",Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"slug": "roboto",
					"name": "Roboto"
				},
				{
					"fontFamily": "\"Roboto Serif\", serif",
					"slug": "roboto-serif",
					"name": "Roboto Serif"
				}
			]
		},
		"blocks": {
			"core/group": {
				"color": {
					"background": true
				}
			},
			"core/column": {
				"color": {
					"background": true
				}
			},
			"core/columns": {
				"color": {
					"background": true
				}
			},
			"core/separator": {
				"color": {
					"background": true
				}
			},
			"core/button": {
				"color": {
					"custom": false
				},
				"border": {
					"radius": false
				}
			},
			"core/paragraph": {
				"color": {
					"custom": false,
					"background": true,
					"text": true,
					"link": true
				},
				"typography": {
					"customFontSize": true
				}
			},
			"core/navigation": {
				"typography": {
					"customFontSize": false
				},
				"spacing": {
					"blockGap": false
				}
			},
			"core/heading": {
				"typography": {
					"customFontSize": false
				},
				"color": {
					"custom": false
				}
			}
		},
		"custom": {
			"line-height": {
				"baseline": 1,
				"small": 1.1,
				"medium": 1.2,
				"large": 1.3,
				"x-large": 1.4,
				"xx-large": 1.5
			},
			"spacing": {
				"x-small": ".555rem",
				"small": ".889rem",
				"baseline": "1.266rem",
				"medium": "1.802rem",
				"large": "2.281rem",
				"outer": "var(--wp--custom--spacing--small, 1.802rem)"
			},
			"border": {
				"radius": "0"
			},
			"color": {
				"link": "var(--wp--preset--color--ucsc-primary-blue)",
				"link-decoration": "var(--wp--preset--color--ucsc-primary-yellow)"
			}
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "var(--wp--preset--font-size--one)",
			"padding": {
				"top": "0",
				"right": "1rem",
				"bottom": "0",
				"left": "1rem"
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--black)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--roboto)",
			"fontSize": "var(--wp--preset--font-size--base)",
			"lineHeight": "var(--wp--custom--line-height--xx-large)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--five)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--four)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--line-height--x-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--three)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--line-height--x-large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--two)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--line-height--xx-large)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--one)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--line-height--xx-large)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--line-height--xx-large)"
				}
			}
		},
		"blocks": {
			"core/heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"lineHeight": "var(--wp--custom--line-height--small)"
				},
				"color": {
					"text": "var(--wp--custom--color--black)"
				},
				"spacing": {
					"margin": {
						"top": ".702em",
						"right": "0",
						"bottom": "0.493em",
						"left": "0"
					}
				}
			},
			"core/site-title": {
				"typography": {
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--line-height--small)",
					"fontSize": "var(--wp--preset--font-size--seven)"
				}
			},
			"core/navigation": {
				"typography": {
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--line-height--small)",
					"fontSize": "var(--wp--preset--font-size--one)"
				},
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--40)"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--small)"
				},
				"color": {
					"text": "var(--wp--preset--color--light-gray)"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"right": "0",
						"bottom": "0.555em",
						"left": "0"
					}
				}
			},
			"core/post-title": {
				"typography": {
					"lineHeight": "var(--wp--custom--line-height--medium)"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"right": "0",
						"bottom": ".493rem",
						"left": "0"
					}
				}
			},
			"core/columns": {
				"spacing": {
					"margin": {
						"top": "0.493rem",
						"right": "0",
						"bottom": "0.642rem",
						"left": "0"
					}
				}
			},
			"core/block-post": {
				"spacing": {
					"margin": {
						"top": "0"
					}
				}
			},
			"core/button": {
				"border": {
					"radius": "100px"
				}
			}
		}
	},
	"customTemplates": []
}