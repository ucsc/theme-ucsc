{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"templateParts": [
		{
			"name": "header",
			"area": "header"
		},
		{
			"name": "footer",
			"area": "footer"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": false,
			"link": false,
			"defaultPalette": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"customDuotone": false,
			"customGradient": false,
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"slug": "white",
					"color": "#fff",
					"name": "White"
				},
				{
					"slug": "black",
					"color": "#000",
					"name": "Black"
				},
				{
					"slug": "ucsc-primary-blue",
					"color": "#003c6c",
					"name": "Primary blue"
				},
				{
					"slug": "ucsc-primary-yellow",
					"color": "#fdc700",
					"name": "Primary yellow"
				},
				{
					"slug": "ucsc-secondary-blue",
					"color": "#006aad",
					"name": "Secondary blue"
				},
				{
					"slug": "ucsc-light-blue",
					"color": "#13a5dc",
					"name": "Light blue"
				},
				{
					"slug": "ucsc-blue-green",
					"color": "#13a5dc",
					"name": "Blue-green"
				},
				{
					"slug": "ucsc-orange",
					"color": "#f29813",
					"name": "UCSC orange"
				},
				{
					"slug": "ucsc-light-orange",
					"color": "#ffbf00",
					"name": "UCSC light orange"
				},
				{
					"slug": "ucsc-lime-green",
					"color": "#93c02d",
					"name": "UCSC lime green"
				},
				{
					"slug": "ucsc-rubine-red",
					"color": "#da216d",
					"name": "UCSC rubine red"
				}
			]
		},
		"layout": {
			"contentSize": "80rem"
		},
		"spacing": {
			"blockGap": true,
			"padding": true,
			"margin": true,
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"typography": {
			"lineHeight": true,
			"customFontSize": false,
			"fontSizes": [
				{
					"slug": "small",
					"size": "clamp(0.8rem, 0.22vw + 0.73rem, 0.9rem)",
					"name": "Small"
				},
				{
					"slug": "base",
					"size": "clamp(1rem, 0.28vw + 0.92rem, 1.13rem)",
					"name": "Base"
				},
				{
					"slug": "medium",
					"size": "clamp(1.25rem, 0.35vw + 1.15rem, 1.41rem)",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "clamp(1.56rem, 0.43vw + 1.43rem, 1.76rem)",
					"name": "Large"
				},
				{
					"slug": "x-large",
					"size": "clamp(1.95rem, 0.54vw + 1.79rem, 2.2rem)",
					"name": "XL"
				},
				{
					"slug": "xx-large",
					"size": "clamp(2.44rem, 0.68vw + 2.24rem, 2.75rem)",
					"name": "XXL"
				},
				{
					"slug": "xxx-large",
					"size": "clamp(3.05rem, 0.85vw + 2.8rem, 3.43rem)",
					"name": "XXXL"
				}
			],
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"slug": "system-font",
					"name": "System Font"
				},
				{
					"fontFamily": "\"Roboto\", sans-serif",
					"slug": "roboto",
					"name": "Roboto"
				},
				{
					"fontFamily": "\"Roboto Condensed\", sans-serif",
					"slug": "roboto-condensed",
					"name": "Roboto Condensed"
				}
			]
		},
		"blocks": {
			"core/button": {
				"color": {
					"custom": false,
					"palette": []
				},
				"border": {
					"radius": false
				}
			},
			"core/paragraph": {
				"color": {
					"custom": false,
					"background": false,
					"text": true,
					"link": false
				},
				"typography": {
					"customFontSize": true
				}
			},
			"core/navigation": {
				"typography": {
					"customFontSize": true
				}
			},
			"core/heading": {
				"typography": {
					"customFontSize": false
				},
				"color": {
					"custom": false,
					"background": false,
					"text": false,
					"link": false
				}
			}
		},
		"custom": {
			"line-height": {
				"tiny": 1.15,
				"small": 1.2,
				"baseline": 1.5,
				"medium": 1.68,
				"large": 1.88
			},
			"spacing": {
				"small": ".85rem",
				"baseline": "1.25rem",
				"medium": "clamp(2rem, 8vw, calc(4 * var(--wp--style--block-gap)))",
				"large": "clamp(4rem, 10vw, 8rem)",
				"outer": "var(--wp--custom--spacing--small, 1.25rem)"
			},
			"border": {
				"radius": "0.25rem"
			},
			"color": {
				"link": "#01589d",
				"link-decoration": "#ffc500",
				"heading-text": "#3e474a",
				"site-title": "#00458c",
				"default-text": "#222222",
				"flourish": "#a2a09c",
				"light-gray": "#555"
			}
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "var(--wp--custom--spacing--small)"
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--custom--color--default-text)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--roboto)",
			"fontSize": "var(--wp--preset--font-size--base)",
			"lineHeight": "var(--wp--custom--line-height--baseline)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--custom--color--link)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "600"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--line-height--tiny)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--base)",
					"fontWeight": "400",
					"lineHeight": "var(--wp--custom--line-height--tiny)"
				}
			}
		},
		"blocks": {
			"core/heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto-condensed)"
				},
				"color": {
					"text": "var(--wp--custom--color--heading-text)"
				},
				"spacing": {
					"margin": {
						"top": ".12em",
						"right": "0",
						"bottom": "0.25em",
						"left": "0"
					}
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto-condensed)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--line-height--small)",
					"textTransform": "uppercase",
					"fontSize": "2.8rem"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"color": {
					"text": "var(--wp--custom--color--light-gray)"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"right": "0",
						"bottom": "0.5em",
						"left": "0"
					}
				}
			},
			"core/post-title": {
				"typography": {
					"lineHeight": "var(--wp--custom--line-height--baseline)"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"right": "0",
						"bottom": ".25rem",
						"left": "0"
					}
				}
			},
			"core/columns": {
				"spacing": {
					"margin": {
						"top": "0.75em",
						"right": "0",
						"bottom": "0.75em",
						"left": "0"
					}
				}
			},
			"core/block-post": {
				"spacing": {
					"margin": {
						"top": "0"
					}
				}
			},
			"core/button": {
				"border": {
					"radius": "var(--wp--custom--border--radius)"
				}
			}
		}
	},
	"customTemplates": []
}
