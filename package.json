{
	"name": "ucsc-2022",
	"version": "1.0.3",
	"description": "The offficial WordPress theme for UC Santa Cruz",
	"main": "build/index.js",
	"scripts": {
		"release": "standard-version",
		"zip": "wp-scripts build && wp-scripts plugin-zip",
		"start": "wp-scripts start",
		"start:hot": "wp-scripts start --hot",
		"build": "wp-scripts build",
		"lint:style": "wp-scripts lint-style 'src/scss/**/*.scss'",
		"packages-update": "wp-scripts packages-update",
		"postpackages-update": "npm run build",
		"format:src": "wp-scripts format ./src"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ucsc/ucsc-2022.git"
	},
	"keywords": [
		"WordPress"
	],
	"author": "UC Santa Cruz",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/ucsc/theme-ucsc/issues"
	},
	"homepage": "https://github.com/ucsc/theme-ucsc#readme",
	"devDependencies": {
		"@wordpress/scripts": "^22.3.0",
		"standard-version": "^9.3.2"
	},
	"files": [
		"screenshot.png",
		"vendor",
		"!vendor/bin",
		"!vendor/squizlabs/php_codesniffer",
		"!vendor/dealerdirect",
		"!vendor/wp-coding-standards",
		"lib",
		"README.md",
		"readme.txt",
		"CHANGELOG.md",
		"build",
		"theme.json",
		"templates",
		"parts",
		"wp-blocks",
		"images",
		"style.css",
		"*.php"
	]
}
